@model IEnumerable<EcomerceWebASP.Models.Types>

@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}

<div id="content" class="">
    <div class="content-main p-4">

        <p>
            @Html.ActionLink("Create New", "Create")
        </p>
        <table class="table">
            <tr>
                <th>
                    Danh mục
                </th>
                <th></th>
            </tr>

            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.nameType)
                    </td>
                    <td>
                        @Html.ActionLink("Edit", "Edit", new { id = item.idType }) |

                        @Html.ActionLink("Delete", "Delete", new { id = item.idType })
                    </td>
                </tr>
            }

        </table>
    </div>
</div>
@Scripts.Render("~/bundles/admin/jquery")
<script>
    $('.d-types').addClass('active');
</script>